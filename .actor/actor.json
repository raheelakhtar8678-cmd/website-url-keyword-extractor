{
    "actorSpecification": 1,
    "name": "keyword-extractor-pro",
    "title": "Keyword Extractor Pro - Advanced SEO Analysis",
    "description": "Extract keywords from any URL with Cloudflare bypass. Supports articles, ecommerce sites, blogs. Generates comprehensive SEO reports with competition analysis, LSI keywords, long-tail keywords, and search suggestions.",
    "version": "1.0.6",
    "meta": {
        "templateId": "node-playwright"
    },
    "input": "./INPUT_SCHEMA.json",
    "dockerfile": "./Dockerfile",
    "readme": "./README.md",
    "categories": [
        "SEO",
        "SCRAPING",
        "ANALYTICS"
    ],
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Keyword Analysis Results",
            "views": {
                "overview": {
                    "title": "Overview",
                    "transformation": {
                        "fields": [
                            "url",
                            "contentType",
                            "totalKeywords",
                            "extractedAt"
                        ]
                    },
                    "display": {
                        "component": "table"
                    }
                }
            }
        }
    }
}